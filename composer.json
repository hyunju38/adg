{
    "name": "php-boilerplate",
    "description": "boilerplate",
    "version": "0.0.1",
    "repositories": [
    ],
    "require": {
        "phpoffice/phpexcel": "^1.8"
    },
    "require-dev": {
        "phpunit/phpunit": "^4.7",
        "squizlabs/php_codesniffer": "^2.3",
        "phpmd/phpmd": "^2.2",
